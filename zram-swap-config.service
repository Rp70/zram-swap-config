[Unit]
Description=zram-swap-config
DefaultDependencies=no
After=local-fs.target log2ram.target basic.target rsyslog.service syslog.target systemd-journald.service sysinit.target shutdown.target

[Service]
Type=oneshot
ExecStart= /usr/local/bin/zram-swap-config start
ExecStop= /usr/local/bin/zram-swap-config stop
TimeoutStartSec=120


[Install]
WantedBy=sysinit.target
